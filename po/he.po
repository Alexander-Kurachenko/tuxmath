msgid "Notice the answer"
msgstr ""

#: src/game.c:800
msgid "If it gets hit again, the"
msgstr ""

#: src/game.c:801
msgid "penguin leaves."
msgstr ""

#: src/game.c:802
msgid "(Press a key when ready)"
msgstr ""

#: src/game.c:821
msgid "You can fix the igloos"
msgstr ""

#: src/game.c:822
msgid "by stopping bonus comets."
msgstr ""

#: src/game.c:831
msgid "Zap it now!"
msgstr ""

#: src/game.c:835
msgid "Great job!"
msgstr ""

#: src/game.c:849
msgid "Quit at any time by pressing"
msgstr ""

#: src/game.c:850
